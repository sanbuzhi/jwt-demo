<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.koushidai.xiaoyuanli.registrationservice.mapper.JWTUserMapper">
    <select id="login" resultType="com.koushidai.xiaoyuanli.registrationservice.pojo.JWTUser">
        select * from xxl_user.jwt where userName=#{userName} and passWord=#{passWord}
    </select>
</mapper>